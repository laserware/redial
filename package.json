{
  "name": "@laserware/redial",
  "version": "1.4.1",
  "description": "Redux IPC abstraction layer.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "url": "git+https://github.com/laserware/redial.git",
    "type": "git"
  },
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && cd dist && npx prettier --write index.d.ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run eslint && npm run typecheck",
    "version": "npm run prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --cache --write src",
    "prettier:ci": "prettier --check src",
    "eslint": "eslint --cache src tsup.config.ts",
    "vitest": "vitest"
  },
  "devDependencies": {
    "@laserware/eslint-config": "^3.0.1",
    "@laserware/stasis": "^3.0.2",
    "electron": "^30.0.6",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@laserware/stasis": ">=3",
    "electron": "*",
    "typescript": ">=5"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
